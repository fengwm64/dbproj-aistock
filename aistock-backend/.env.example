# Docker环境变量示例文件
# 复制此文件为.env并填入实际值

# 数据库配置
DATABASE_URI=mysql+pymysql://aistock:password@db:3306/aistock_db
MYSQL_ROOT_PASSWORD=rootpassword
MYSQL_DATABASE=aistock_db
MYSQL_USER=aistock
MYSQL_PASSWORD=password

# JWT配置
JWT_SECRET_KEY=your-jwt-secret-key-here

# 微信配置
WECHAT_APPID=your-wechat-appid
WECHAT_SECRET=your-wechat-secret
WECHAT_TEMPLATE_ID=your-template-id

# Redis配置
REDIS_BROKER_URL=redis://redis:6379/0

# 微信扫码配置
WECHAT_SCAN_APPID=your-scan-appid
WECHAT_SCAN_SECRET=your-scan-secret
WECHAT_SCAN_TOKEN=your-scan-token
